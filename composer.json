{
    "name": "amelaraby/magento-2-mageplaza-banner-slider-api",
    "description": "An extension for Magento 2 Mageplaza BannerSlider extension which expose an APIs to fetch slider and banners data",
    "require": {
        "mageplaza/module-banner-slider": "^4.0.0",
        "magento/magento-composer-installer": "*"
    },
    "authors": [{
        "name": "Ahmed Elaraby",
        "email": "araby2305@gmail.com",
        "role": "Technical Lead"
    }],
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "proprietary",
    "keywords": [
        "magento 2",
        "banner slider",
        "magento 2 banner slider",
        "magento 2 banner slider api",
        "slider api"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Logixie\\MageplazaBannerSliderApi\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "Logixie/MageplazaBannerSliderApi"
            ]
        ]
    }
}
